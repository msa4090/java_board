package home;

public class DetailpageClass {
	private String comments;	// 댓글
	private int commnets_index; // 댓글 번호
	DetailpageClass[][] commentArr = new DetailpageClass[100][2];
	
	public DetailpageClass[][] getCommentArr() {
		return commentArr;
	}

	public void setCommentArr(DetailpageClass[][] commentArr) {
		this.commentArr = commentArr;
	}

	public int getCommnets_index() {
		return commnets_index;
	}

	public void setCommnets_index(int commnets_index) {
		this.commnets_index = commnets_index;
	}

	public String getComments() {
		return comments;
	}
	
	public void setComments(String comments) {
		this.comments = comments;
	}
}
